<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        
    </style>
    
    <style name="RaisedButtonDark" parent="Theme.AppCompat.Light">
        <item name="colorButtonNormal">@color/colorPrimaryDark</item>
        <item name="colorControlHighlight">@color/colorPrimary</item>
        <item name="android:textColor">@android:color/white</item>


    </style>

    <style name="EditTextblue" parent="TextAppearance.AppCompat">
        <item name="android:textColorHighlight">@color/editTextColor</item>
        <item name="android:textSize">30sp</item>
        <item name="android:textColorPrimary">@color/editTextColor</item>
        <item name="android:colorAccent">@color/editTextColor</item>
        <item name="android:colorControlActivated">@color/editTextColor</item>




    </style>

</resources>
